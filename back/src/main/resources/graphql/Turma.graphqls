type Turma {
    codigo: String!
    vagasOfertadas: Int!
    nomeProfessor: String!
    disciplina: Disciplina!
    horarios: [HorarioTurma!]!
}

type HorarioTurma {
    diaSemana: DiaSemana!
    horarioInicio: HorarioAula!
    horarioFinal: HorarioAula!
    sala: String!
}

type Query {
    turmas: [Turma!]!
}
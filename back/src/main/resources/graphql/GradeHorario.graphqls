type HorarioAula {
    id: Int!
    horario: String!
}

type DiaSemana {
    id: Int!
    nome: String!
}

type HorarioTurmaMap {
    horarioId: Int!
    horariosPorDiaSemana: [HorarioPorDiaSemana!]!
}

type HorarioPorDiaSemana {
    diaSemanaId: Int!
    horariosTurma: [HorarioTurma!]!
}

extend type Query {
    horarios: [HorarioAula!]!
    diasSemana: [DiaSemana!]!
    turmasGradeHorario: [HorarioTurmaMap!]!
}